{
    "name": "porthd/mysearchext",
    "type": "typo3-cms-extension",
    "description": "TYPO3-extension for the usage of a local search-index, generated by th individual searching in the net",
	"keywords": ["typo3", "extension", "composer.json"],
	"license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Dr. Dieter Porth",
            "role": "Developer",
			"email": "info@mobger.de"
        }
    ],
    "require": {
		"ext-json": "*",
		"typo3/cms-core": "^11.5",
		"typo3/cms-frontend": "^11.5",
		"typo3/cms-scheduler": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Porthd\\Mysearchext\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Porthd\\Mysearchext\\Tests\\": "Tests"
        }
    },
    "replace": {
        "porthd/mysearchext": "self.version",
        "typo3-ter/mysearchext": "self.version"
    },
	"extra": {
		"typo3/cms": {
			"extension-key": "mysearchext"
		}
	}
}
